{
  "alias": ["pakketadvies.nl", "www.pakketadvies.nl"],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "crons": [
    {
      "path": "/api/cron/update-dynamic-prices",
      "schedule": "0 14 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.pakketadvies.nl"
        }
      ],
      "destination": "https://pakketadvies.nl/$1",
      "permanent": true
    }
  ]
}



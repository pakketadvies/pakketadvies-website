{
  "alias": ["pakketadvies.nl", "www.pakketadvies.nl"],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "crons": [
    {
      "path": "/api/cron/update-dynamic-prices",
      "schedule": "0 14 * * *"
    }
  ],
  "redirects": [
    {
      "source": "https://www.pakketadvies.nl/:path*",
      "destination": "https://pakketadvies.nl/:path*",
      "permanent": true
    }
  ]
}


